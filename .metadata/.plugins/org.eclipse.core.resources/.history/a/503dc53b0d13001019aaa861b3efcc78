package com.app.ui;

import java.util.Scanner;

public class Test {

	public static void main(String[] args) {
		try (Scanner sc = new Scanner(System.in)) {
			boolean exit = false;
			while (!exit) {
				switch (sc.nextInt()){
				case 1:
						
					break;

				default:
					break;
				}
			}
		} catch (Exception e) {
			System.out.println(e);
		}
	}

}
